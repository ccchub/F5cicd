# Provision a BYOL VE on Azure
./deploy_via_bash.sh  --adminUsername azureuser --authenticationType sshPublicKey --adminPasswordOrKey 'ssh-rsa  Chris@MBP.local' --dnsLabel sentinel --instanceName f5vm01 --numberOfExternalIps 1 --instanceType Standard_DS3_v2 --imageName AllTwoBootLocations --bigIpVersion 14.1.200000 --bigIpModules ltm:nominal,asm:nominal --licenseKey1 YWDMV-SMKDM-EBOLR-NTDHH-PAPKFEV --vnetAddressPrefix 10.0 --declarationUrl NOT_SPECIFIED --ntpServer 0.pool.ntp.org --timeZone UTC --customImage OPTIONAL --allowUsageAnalytics Yes --resourceGroupName cz_sentinel

