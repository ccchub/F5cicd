# Create RG and NSG

az network nsg create -g cz-lb-fw-4vnets-2 -n 4vnets-apply-all-nsg --location eastus2
az network nsg rule create -g cz-lb-fw-4vnets-2 --nsg-name 4vnets-apply-all-nsg -n Allow_All --priority 100 \
                            --source-address-prefixes 69.251.248.77/32 --source-port-ranges '*' \
                            --destination-address-prefixes '*' --destination-port-ranges '*' --access Allow \
                            --protocol '*'

az network nsg create -g cz-lb-fw-4vnets-2 -n 4vnets-apply-all-nsg2 --location eastus2
az network nsg rule create -g cz-lb-fw-4vnets-2 --nsg-name 4vnets-apply-all-nsg2 -n Allow_All --priority 100 \
                            --source-address-prefixes 69.251.248.77/32 --source-port-ranges '*' \
                            --destination-address-prefixes '*' --destination-port-ranges '*' --access Allow \
                            --protocol '*'





