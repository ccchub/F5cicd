# udr-d2
az network route-table create --name udr-d2 --resource-group cz-lb-fw-4vnets-2 --location eastus2
az network vnet subnet update --resource-group cz-lb-fw-4vnets-2 --vnet-name cz-d2 --name d2-subnet --route-table udr-d2
az network route-table route create --resource-group cz-lb-fw-4vnets-2 --route-table-name udr-d2 --name default --next-hop-type VirtualAppliance --address-prefix 0.0.0.0/0 --next-hop-ip-address 10.3.20.245
az network route-table route create --resource-group cz-lb-fw-4vnets-2 --route-table-name udr-d2 --name 69.251.248.77 --next-hop-type Internet --address-prefix 69.251.248.77/32


# udr-d3
az network route-table create --name udr-d3 --resource-group cz-lb-fw-4vnets-2 --location eastus2
az network vnet subnet update --resource-group cz-lb-fw-4vnets-2 --vnet-name cz-d3 --name d3-subnet --route-table udr-d3
az network route-table route create --resource-group cz-lb-fw-4vnets-2 --route-table-name udr-d3 --name default --next-hop-type VirtualAppliance --address-prefix 0.0.0.0/0 --next-hop-ip-address 10.3.30.245
az network route-table route create --resource-group cz-lb-fw-4vnets-2 --route-table-name udr-d3 --name 69.251.248.77 --next-hop-type Internet --address-prefix 69.251.248.77/32

