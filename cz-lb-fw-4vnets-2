# Create d2, d3, d4 and d5

az network vnet create -g cz-lb-fw-4vnets-2 -n cz-d2 --address-prefix 172.16.20.0/24
az network vnet create -g cz-lb-fw-4vnets-2 -n cz-d3 --address-prefix 172.16.30.0/24
az network vnet create -g cz-lb-fw-4vnets-2 -n cz-d4 --address-prefix 172.16.40.0/24
az network vnet create -g cz-lb-fw-4vnets-2 -n cz-d5 --address-prefix 172.16.50.0/24

az network vnet subnet create -g cz-lb-fw-4vnets-2 --vnet-name cz-d2 -n d2-subnet --address-prefix 172.16.20.0/24 --network-security-group 4vnets-apply-all-nsg
az network vnet subnet create -g cz-lb-fw-4vnets-2 --vnet-name cz-d3 -n d3-subnet --address-prefix 172.16.30.0/24 --network-security-group 4vnets-apply-all-nsg
