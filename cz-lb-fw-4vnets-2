az network public-ip create --resource-group cz-lb-fw-4vnets-2 --name d3-vm-pub-ip --allocation-method static
az network public-ip create --resource-group cz-lb-fw-4vnets-2 --name d4-vm-pub-ip --allocation-method static
az network public-ip create --resource-group cz-lb-fw-4vnets-2 --name d5-vm-pub-ip --allocation-method static

