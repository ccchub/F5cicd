{
    "$schema": "https://raw.githubusercontent.com/F5Networks/f5-appsvcs-extension/master/schema/3.14.0/as3-schema-3.14.0-4.json",
    "class": "AS3",
    "action": "deploy",
    "declaration": {
        "class": "ADC",
        "schemaVersion": "3.12.0",
        "target": {
            "address": "192.168.1.199"
        },
        "AS3": {
            "class": "Tenant",
            "DVWA": {
                "class": "Application",
                "schemaOverlay": "AS3-F5-HTTPS-WAF-existing-lb-template-big-iq-default-v1",
                "template": "https",
                "serviceMain": {
                    "profileAnalytics": {
                        "use": "Analytics_Profile"
                    },
                    "virtualAddresses": [
                        "192.168.1.21"
                    ],
                    "enable": true,
                    "pool": "Pool",
                    "virtualPort": 443,
                    "policyWAF": {
                        "bigip": "/Common/DVWA_WAF_pol.app/DVWA_WAF_pol"
                    },
                    "serverTLS": "TLS_Server",
                    "securityLogProfiles": [
                        {
                            "bigip": "/Common/asm-remote-logging-profile"
                        }
                    ],
                    "class": "Service_HTTPS"
                },
                "Analytics_Profile": {
                    "collectUserAgent": true,
                    "collectClientSideStatistics": true,
                    "collectGeo": true,
                    "collectUrl": true,
                    "collectPageLoadTime": true,
                    "collectResponseCode": true,
                    "collectIp": true,
                    "class": "Analytics_Profile"
                },
                "Pool": {
                    "members": [
                        {
                            "servicePort": 80,
                            "adminState": "enable",
                            "serverAddresses": [
                                "172.16.18.82"
                            ]
                        }
                    ],
                    "class": "Pool"
                },
                "Certificate": {
                    "certificate": {
                        "bigip": "/Common/sha2_wildcard.home.lab.crt"
                    },
                    "privateKey": {
                        "bigip": "/Common/sha2_wildcard.home.lab.key"
                    },
                    "class": "Certificate"
                },
                "TLS_Server": {
                    "certificates": [
                        {
                            "certificate": "Certificate"
                        }
                    ],
                    "class": "TLS_Server"
                }
            }
        },
        "id": "SfqkFGOh-TNeo-rKMf-vNoS-pMCLQLJJZQHc"
    }
}